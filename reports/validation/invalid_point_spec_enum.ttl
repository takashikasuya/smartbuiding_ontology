@prefix rec: <https://w3id.org/rec/> .
@prefix sbco: <https://www.sbco.or.jp/ont/> .

sbco:point_temp_007 a sbco:PointExt ;
    rec:isPartOf sbco:room_demo_701 ;
    rec:name "Room Temperature" ;
    sbco:id "sbco:point_temp_007" ;
    sbco:pointSpecification "NotAType" ;
    sbco:pointType "TemperatureSensor" ;
    sbco:unit "celsius" .

sbco:site_demo_007 a rec:Site ;
    rec:hasPart sbco:building_demo_007 ;
    rec:name "Demo Site Invalid Point Spec" ;
    sbco:id "sbco:site_demo_007" .

sbco:building_demo_007 a rec:Building ;
    rec:hasPart sbco:level_demo_7f ;
    rec:isPartOf sbco:site_demo_007 ;
    rec:name "Demo Building" ;
    sbco:id "sbco:building_demo_007" .

sbco:level_demo_7f a rec:Level ;
    rec:hasPart sbco:room_demo_701 ;
    rec:isPartOf sbco:building_demo_007 ;
    rec:name "7F" ;
    sbco:id "sbco:level_demo_7f" .

sbco:room_demo_701 a rec:Room ;
    rec:hasPoint sbco:point_temp_007 ;
    rec:isPartOf sbco:level_demo_7f ;
    rec:name "Demo Room" ;
    sbco:id "sbco:room_demo_701" .

