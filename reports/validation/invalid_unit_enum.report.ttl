@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sbco: <https://www.sbco.or.jp/ont/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

[] a sh:ValidationReport ;
    sh:conforms false ;
    sh:result [ a sh:ValidationResult ;
            sh:focusNode sbco:point_temp_003 ;
            sh:resultMessage "Value Literal(\"invalidUnit\") not in list ['Literal(\"ppm\")', 'Literal(\"percent\")', 'Literal(\"celsius\")']" ;
            sh:resultPath sbco:unit ;
            sh:resultSeverity sh:Violation ;
            sh:sourceConstraintComponent sh:InConstraintComponent ;
            sh:sourceShape [ sh:description "Measurement unit (enum key; symbol can be taken from annotations)" ;
                    sh:in ( "celsius" "percent" "ppm" ) ;
                    sh:maxCount 1 ;
                    sh:order 2 ;
                    sh:path sbco:unit ] ;
            sh:value "invalidUnit" ] .

