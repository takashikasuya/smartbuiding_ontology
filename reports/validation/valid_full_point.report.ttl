@prefix sbco: <https://www.sbco.or.jp/ont/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

[] a sh:ValidationReport ;
    sh:conforms false ;
    sh:result [ a sh:ValidationResult ;
            sh:focusNode sbco:point_temp_005 ;
            sh:resultMessage "Value is not Literal with datatype xsd:float" ;
            sh:resultPath sbco:minPresValue ;
            sh:resultSeverity sh:Violation ;
            sh:sourceConstraintComponent sh:DatatypeConstraintComponent ;
            sh:sourceShape [ sh:datatype xsd:float ;
                    sh:description "Minimum plausible reading" ;
                    sh:maxCount 1 ;
                    sh:nodeKind sh:Literal ;
                    sh:order 4 ;
                    sh:path sbco:minPresValue ] ;
            sh:value 1e+01 ],
        [ a sh:ValidationResult ;
            sh:focusNode sbco:point_temp_005 ;
            sh:resultMessage "Value is not Literal with datatype xsd:float" ;
            sh:resultPath sbco:maxPresValue ;
            sh:resultSeverity sh:Violation ;
            sh:sourceConstraintComponent sh:DatatypeConstraintComponent ;
            sh:sourceShape [ sh:datatype xsd:float ;
                    sh:description "Maximum plausible reading" ;
                    sh:maxCount 1 ;
                    sh:nodeKind sh:Literal ;
                    sh:order 3 ;
                    sh:path sbco:maxPresValue ] ;
            sh:value 3.5e+01 ],
        [ a sh:ValidationResult ;
            sh:focusNode sbco:point_temp_005 ;
            sh:resultMessage "Value is not Literal with datatype xsd:float" ;
            sh:resultPath sbco:scale ;
            sh:resultSeverity sh:Violation ;
            sh:sourceConstraintComponent sh:DatatypeConstraintComponent ;
            sh:sourceShape [ sh:datatype xsd:float ;
                    sh:description "Scale factor for raw value conversion" ;
                    sh:maxCount 1 ;
                    sh:nodeKind sh:Literal ;
                    sh:order 5 ;
                    sh:path sbco:scale ] ;
            sh:value 1e+00 ] .

