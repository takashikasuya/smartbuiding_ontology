@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rec: <https://w3id.org/rec/> .
@prefix sbco: <https://www.sbco.or.jp/ont/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

linkml:Any a rdfs:Datatype ;
    owl:equivalentClass xsd:string .

sbco:Building a owl:Class ;
    rdfs:label "Building" ;
    rdfs:comment "A building which is part of a site." ;
    rdfs:subClassOf sbco:Architecture ;
    skos:exactMatch rec:Building ;
    skos:inScheme sbco:schema ;
    sbco:description_ja "サイトの一部である建物。" .

sbco:Image a owl:Class ;
    rdfs:label "Image" ;
    rdfs:comment "An image file containing visual information" ;
    rdfs:subClassOf sbco:Information ;
    skos:exactMatch rec:Image ;
    skos:inScheme sbco:schema ;
    sbco:description_ja "視覚情報を含む画像ファイル" .

sbco:Level a owl:Class ;
    rdfs:label "Level" ;
    rdfs:comment "A building storey. Levels can also be treated as a kind of space." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:levelNumber ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:integer ;
            owl:onProperty sbco:levelNumber ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:levelNumber ],
        sbco:Architecture ;
    skos:exactMatch rec:Level ;
    skos:inScheme sbco:schema ;
    sbco:description_ja "建物のフロア。スペースの一種として扱える。" .

sbco:Media a owl:Class ;
    rdfs:label "Media" ;
    rdfs:comment "A media file such as audio or video content" ;
    rdfs:subClassOf sbco:Information ;
    skos:exactMatch rec:Media ;
    skos:inScheme sbco:schema ;
    sbco:description_ja "オーディオやビデオコンテンツなどのメディアファイル" .

sbco:OutdoorSpace a owl:Class ;
    rdfs:label "OutdoorSpace" ;
    rdfs:comment "An outdoor space associated with a site or building." ;
    rdfs:subClassOf sbco:Architecture ;
    skos:exactMatch rec:OutdoorSpace ;
    skos:inScheme sbco:schema .

sbco:Room a owl:Class ;
    rdfs:label "Room" ;
    rdfs:comment "A room within a building." ;
    rdfs:subClassOf sbco:Architecture ;
    skos:exactMatch rec:Room ;
    skos:inScheme sbco:schema .

sbco:Schema a owl:Class ;
    rdfs:label "Schema" ;
    rdfs:comment "A schema definition file" ;
    rdfs:subClassOf sbco:Information ;
    skos:exactMatch rec:Schema ;
    skos:inScheme sbco:schema ;
    sbco:description_ja "スキーマ定義ファイル" .

sbco:Site a owl:Class ;
    rdfs:label "Site" ;
    rdfs:comment "A piece of land upon which zero or more buildings may be situated." ;
    rdfs:subClassOf sbco:Architecture ;
    skos:exactMatch rec:Site ;
    skos:inScheme sbco:schema ;
    sbco:description_ja "サイト。複数の建物を含みうる。" .

sbco:Zone a owl:Class ;
    rdfs:label "Zone" ;
    rdfs:comment "A sub-zone within or outside of a building defined to support some technology and/or use." ;
    rdfs:subClassOf sbco:Architecture ;
    skos:exactMatch rec:Zone ;
    skos:inScheme sbco:schema .

sbco:altitude a owl:DatatypeProperty ;
    rdfs:label "altitude" ;
    rdfs:comment "Altitude above sea level in meters" ;
    rdfs:range xsd:float ;
    skos:inScheme sbco:schema ;
    sbco:description_ja "海抜高度（メートル）" .

sbco:coordinates a owl:DatatypeProperty ;
    rdfs:label "coordinates" ;
    rdfs:comment "Geographic coordinates defining the geofence boundary" ;
    rdfs:range xsd:string ;
    skos:inScheme sbco:schema ;
    sbco:description_ja "ジオフェンス境界を定義する地理座標" .

sbco:intervalCapability a owl:DatatypeProperty ;
    rdfs:label "intervalCapability" ;
    rdfs:comment "Polling period (period speed that the device is capable of)" ;
    rdfs:range xsd:integer ;
    skos:inScheme sbco:schema .

sbco:radius a owl:DatatypeProperty ;
    rdfs:label "radius" ;
    rdfs:comment "Radius in meters for circular geofences" ;
    rdfs:range xsd:float ;
    skos:inScheme sbco:schema ;
    sbco:description_ja "円形ジオフェンスの半径（メートル）" .

sbco:schema.owl.ttl a owl:Ontology ;
    rdfs:label "building_model" ;
    dcterms:title "Smart Building Model" ;
    rdfs:comment """A LinkML schema for sites, buildings, spaces, equipment, and points used in smart-building data exchanges.
""" .

sbco:Asset a owl:Class ;
    rdfs:label "Asset" ;
    rdfs:comment "Something which is placed inside of a building, but is not an integral part of that building's structure; e.g., furniture, equipment, systems, etc." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:mountedOn ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:KeyStringMapEntry ;
            owl:onProperty sbco:identifiers ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:Agent ;
            owl:onProperty sbco:commissionedBy ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:geometry ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty sbco:assetTag ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:modelNumber ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty sbco:modelNumber ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:customProperties ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:commissionedBy ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty sbco:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:initialCost ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty sbco:name ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty sbco:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:installationDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty sbco:serialNumber ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:Agent ;
            owl:onProperty sbco:manufacturedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:KeyBoolMapEntry ;
            owl:onProperty sbco:customTags ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( xsd:string [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})$" ] ) ] ) ] ;
            owl:onProperty sbco:MACAddress ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty sbco:description ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty sbco:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:date ;
            owl:onProperty sbco:turnoverDate ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:servicedBy ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:MACAddress ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:documentation ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:installedBy ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:KeyMapOfStringMapEntry ;
            owl:onProperty sbco:customProperties ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:IPAddress ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:assetTag ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:Agent ;
            owl:onProperty sbco:installedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:duration ;
            owl:onProperty sbco:maintenanceInterval ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:hasPart ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:Space ;
            owl:onProperty sbco:hasPart ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty sbco:weight ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:maintenanceInterval ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:date ;
            owl:onProperty sbco:commissioningDate ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:serialNumber ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:Document ;
            owl:onProperty sbco:documentation ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:Geometry ;
            owl:onProperty sbco:geometry ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:manufacturedBy ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:installationDate ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:isPartOf ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ owl:intersectionOf ( [ owl:unionOf ( sbco:Point sbco:PointExt ) ] sbco:Point ) ] ;
            owl:onProperty sbco:hasPoint ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:modelNumber ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:turnoverDate ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:weight ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:date ;
            owl:onProperty sbco:installationDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:Space ;
            owl:onProperty sbco:locatedIn ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:BuildingElement ;
            owl:onProperty sbco:mountedOn ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:serialNumber ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty sbco:initialCost ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:customTags ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:commissioningDate ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:geometry ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:initialCost ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:turnoverDate ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:isPartOf ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:commissioningDate ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:mountedOn ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:hasPoint ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:locatedIn ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:weight ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:Space ;
            owl:onProperty sbco:isPartOf ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:Agent ;
            owl:onProperty sbco:servicedBy ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:identifiers ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( xsd:string [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^((25[0-5]|2[0-4]\\d|1?\\d?\\d)\\.){3}(25[0-5]|2[0-4]\\d|1?\\d?\\d)$" ] ) ] ) ] ;
            owl:onProperty sbco:IPAddress ],
        sbco:Resource ;
    skos:exactMatch rec:Asset ;
    skos:inScheme sbco:schema ;
    sbco:description_ja "建物内に配置されるが、建物構造の不可欠な部分ではないもの。例：家具、設備、システムなど。" .

xsd:duration a rdfs:Datatype ;
    owl:equivalentClass [ a rdfs:Datatype ;
            owl:intersectionOf ( xsd:string [ a rdfs:Datatype ;
                        owl:onDatatype xsd:string ;
                        owl:withRestrictions ( [ xsd:pattern "^P(?:\\d+Y)?(?:\\d+M)?(?:\\d+D)?(?:T(?:\\d+H)?(?:\\d+M)?(?:\\d+(?:\\.\\d+)?S)?)?$" ] ) ] ) ] .

