sbco:ArchitectureArea a owl:Class ;
    rdfs:label "ArchitectureArea" ;
    rdfs:comment "Describes business-relevant area measurements typically associated with architected spaces" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom sbco:KeyBoolMapEntry ;
            owl:onProperty sbco:customTags ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sbco:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty sbco:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:KeyMapOfStringMapEntry ;
            owl:onProperty sbco:customProperties ],
        [ a owl:Restriction ;
            owl:allValuesFrom sbco:KeyStringMapEntry ;
            owl:onProperty sbco:identifiers ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty sbco:name ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty sbco:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty sbco:name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:identifiers ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:customProperties ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sbco:customTags ],
        sbco:Resource ;
    skos:exactMatch rec:ArchitectureArea ;
    skos:inScheme sbco:schema ;
    sbco:description_ja "設計された空間に関連する業務上重要な面積測定" .

