sbco:IPAddress a owl:DatatypeProperty ;
    rdfs:label "IPAddress" ;
    rdfs:comment "IP address of the asset" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:intersectionOf ( xsd:string [ a rdfs:Datatype ;
                        owl:onDatatype xsd:string ;
                        owl:withRestrictions ( [ xsd:pattern "^((25[0-5]|2[0-4]\\d|1?\\d?\\d)\\.){3}(25[0-5]|2[0-4]\\d|1?\\d?\\d)$" ] ) ] ) ] ;
    skos:inScheme sbco:schema .

sbco:MACAddress a owl:DatatypeProperty ;
    rdfs:label "MACAddress" ;
    rdfs:comment "MAC address of the asset" ;
    rdfs:range [ a rdfs:Datatype ;
