            owl:onProperty sbco:identifiers ],
        sbco:Resource ;
    skos:exactMatch rec:ArchitectureCapacity ;
    skos:inScheme sbco:schema ;
    sbco:description_ja "設計された空間に関連する業務上重要な容量測定" .

sbco:Georeference a owl:Class ;
    rdfs:label "Georeference" ;
    rdfs:comment "REC Georeference のプレースホルダ（詳細は別途拡張）" ;
