        [ sh:class sbco:KeyBoolMapEntry ;
            sh:description "map(string -> boolean)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 21 ;
            sh:path rec:customTags ],
        [ sh:class rec:Agent ;
            sh:description "Agent or resource that architected this structure" ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path rec:architectedBy ],
        [ sh:class rec:Space ;
            sh:description "The subject is composed in part of the entity given by the object." ;
            sh:nodeKind sh:IRI ;
            sh:order 17 ;
