@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rec: <https://w3id.org/rec/> .
@prefix sbco: <https://www.sbco.or.jp/ont/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

rdfs:ResourceShape a sh:NodeShape ;
    sh:closed false ;
    sh:description "Base class for all resources" ;
    sh:ignoredProperties ( sbco:targetArea rec:weight rdf:type rec:documentation rec:hasPart rec:owns rec:description rec:identifiers rec:url rec:serialNumber rec:locatedIn rec:name rec:geometry rec:version brick:operationalStageCount sbco:installationArea rec:modelNumber sbco:deviceType rec:checksum rec:memberOf sbco:id rec:turnoverDate rec:customTags rec:format rec:commissioningDate rec:hasPoint rec:isPartOf rec:manufacturedBy brick:feeds rec:size rec:assetTag rec:commissionedBy rec:installationDate sbco:panel rec:maintenanceInterval rec:MACAddress rec:initialCost rec:IPAddress rec:installedBy rec:isFedBy rec:mountedOn rec:language rec:servicedBy rec:customProperties ) ;
    sh:targetClass rdfs:Resource .

brick:EquipmentShape a sh:NodeShape ;
    sh:closed false ;
    sh:description "An equipment asset installed in a space." ;
    sh:ignoredProperties ( sbco:targetArea sbco:panel rdf:type sbco:installationArea sbco:deviceType ) ;
    sh:property [ sh:class rec:Agent ;
            sh:description "Agent or resource that commissioned this asset" ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path rec:commissionedBy ],
        [ sh:maxCount 1 ;
            sh:or ( [ sh:class rec:Space ] [ sh:class rec:Site ] [ sh:class rec:Building ] [ sh:class rec:Level ] [ sh:class rec:Room ] [ sh:class rec:Zone ] [ sh:class rec:OutdoorSpace ] ) ;
            sh:order 11 ;
            sh:path rec:isPartOf ],
        [ sh:description "Point associated with this architecture" ;
            sh:or ( [ sh:class brick:Point ] [ sh:class sbco:PointExt ] ) ;
            sh:order 9 ;
            sh:path rec:hasPoint ],
        [ sh:datatype xsd:decimal ;
            sh:description "Weight of the asset" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 30 ;
            sh:path rec:weight ],
        [ sh:class sbco:KeyBoolMapEntry ;
            sh:description "map(string -> boolean)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 19 ;
            sh:path rec:customTags ],
        [ sh:datatype xsd:string ;
            sh:description "IP address of the asset" ;
            sh:nodeKind sh:Literal ;
            sh:order 23 ;
            sh:path rec:IPAddress ;
            sh:pattern "^((25[0-5]|2[0-4]\\d|1?\\d?\\d)\\.){3}(25[0-5]|2[0-4]\\d|1?\\d?\\d)$" ],
        [ sh:class rdfs:Resource ;
            sh:description "Equipment or system that this equipment feeds" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path brick:feeds ],
        [ sh:datatype sbco:DurationString ;
            sh:description "Maintenance interval duration" ;
            sh:nodeKind sh:Literal ;
            sh:order 25 ;
            sh:path rec:maintenanceInterval ;
            sh:pattern "^P(?:\\d+Y)?(?:\\d+M)?(?:\\d+D)?(?:T(?:\\d+H)?(?:\\d+M)?(?:\\d+(?:\\.\\d+)?S)?)?$" ],
        [ sh:class rec:Agent ;
            sh:description "Agent or resource that installed this asset" ;
            sh:nodeKind sh:IRI ;
            sh:order 10 ;
            sh:path rec:installedBy ],
        [ sh:class rec:Agent ;
            sh:description "Agent or resource that manufactured this asset" ;
            sh:nodeKind sh:IRI ;
            sh:order 13 ;
            sh:path rec:manufacturedBy ],
        [ sh:datatype xsd:date ;
            sh:description "Date when the asset was installed" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 22 ;
            sh:path rec:installationDate ],
        [ sh:class sbco:KeyMapOfStringMapEntry ;
            sh:description "map(string -> map(string -> string))" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 18 ;
            sh:path rec:customProperties ],
        [ sh:datatype xsd:string ;
            sh:description "A textual description of the resource" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path rec:description ],
        [ sh:datatype xsd:date ;
            sh:description "Date when the asset was turned over" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 29 ;
            sh:path rec:turnoverDate ],
        [ sh:datatype xsd:string ;
            sh:description "Unique identifier within the schema. Must start with a letter and contain only letters, digits, underscores, hyphens, colons, semicolons, or periods. DTMI is one acceptable example." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path sbco:id ],
        [ sh:description "Space where this asset is located" ;
            sh:or ( [ sh:class rec:Space ] [ sh:class rec:Site ] [ sh:class rec:Building ] [ sh:class rec:Level ] [ sh:class rec:Room ] [ sh:class rec:Zone ] [ sh:class rec:OutdoorSpace ] ) ;
            sh:order 12 ;
            sh:path rec:locatedIn ],
        [ sh:datatype xsd:integer ;
            sh:description "The number of operational stages supported by this equipment" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path brick:operationalStageCount ],
        [ sh:datatype xsd:string ;
            sh:description "Machine or Human-readable name" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 27 ;
            sh:path rec:name ],
        [ sh:datatype xsd:string ;
            sh:description "MAC address of the asset" ;
            sh:nodeKind sh:Literal ;
            sh:order 24 ;
            sh:path rec:MACAddress ;
            sh:pattern "^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})$" ],
        [ sh:class sbco:KeyStringMapEntry ;
            sh:description "map(string -> string)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 20 ;
            sh:path rec:identifiers ],
        [ sh:datatype xsd:string ;
            sh:description "Initial cost of the asset" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 21 ;
            sh:path rec:initialCost ],
        [ sh:datatype xsd:string ;
            sh:description "Asset identification tag" ;
            sh:nodeKind sh:Literal ;
            sh:order 16 ;
            sh:path rec:assetTag ],
        [ sh:datatype xsd:string ;
            sh:description "Model number of the asset" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 26 ;
            sh:path rec:modelNumber ],
        [ sh:datatype xsd:string ;
            sh:description "Serial number of the asset" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 28 ;
            sh:path rec:serialNumber ],
        [ sh:class rec:Document ;
            sh:description "Documentation related to this asset" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path rec:documentation ],
        [ sh:class rec:BuildingElement ;
            sh:description "Building element on which this asset is mounted" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 14 ;
            sh:path rec:mountedOn ],
        [ sh:class rec:Space ;
            sh:description "The subject is composed in part of the entity given by the object." ;
            sh:nodeKind sh:IRI ;
            sh:order 8 ;
            sh:path rec:hasPart ],
        [ sh:datatype xsd:date ;
            sh:description "Date when the asset was commissioned" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 17 ;
            sh:path rec:commissioningDate ],
        [ sh:class rec:Agent ;
            sh:description "Agent or resource that services this asset" ;
            sh:nodeKind sh:IRI ;
            sh:order 15 ;
            sh:path rec:servicedBy ],
        [ sh:class rec:Geometry ;
            sh:description "Polygon representing the spatial extent of this Space." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 7 ;
            sh:path rec:geometry ],
        [ sh:class rdfs:Resource ;
            sh:description "Resource that feeds this architecture" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path rec:isFedBy ] ;
    sh:targetClass brick:Equipment .

brick:PointShape a sh:NodeShape ;
    sh:closed false ;
    sh:description "A sensor, actuator, or data point associated with equipment." ;
    sh:ignoredProperties ( sbco:pointSpecification rdf:type sbco:minPresValue sbco:scale sbco:unit sbco:maxPresValue sbco:pointType ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Machine or Human-readable name" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path rec:name ],
        [ sh:description "Aggregation function or method for point data processing" ;
            sh:in ( "average" "sum" "minimum" "maximum" "count" "first" "last" "median" "standardDeviation" ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path brick:aggregate ],
        [ sh:class sbco:KeyStringMapEntry ;
            sh:description "map(string -> string)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 7 ;
            sh:path rec:identifiers ],
        [ sh:class sbco:KeyMapOfStringMapEntry ;
            sh:description "map(string -> map(string -> string))" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path rec:customProperties ],
        [ sh:datatype xsd:string ;
            sh:description "Unique identifier within the schema. Must start with a letter and contain only letters, digits, underscores, hyphens, colons, semicolons, or periods. DTMI is one acceptable example." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path sbco:id ],
        [ sh:description "Substance associated with this point" ;
            sh:in ( "ACElec" "Air" "BlowdownWater" "ChilledWater" "ColdDomesticWater" "Condensate" "CondenserWater" "DCElec" "Diesel" "DriveElec" "Ethernet" "ExhaustAir" "Freight" "FuelOil" "Gasoline" "GreaseExhaustAir" "HotDomesticWater" "HotWater" "IrrigationWater" "Light" "MakeupWater" "NaturalGas" "NonPotableDomesticWater" "OutsideAir" "People" "Propane" "RecircHotDomesticWater" "Refrig" "ReturnAir" "SprinklerWater" "Steam" "StormDrainage" "SupplyAir" "TransferAir" "WasteVentDrainage" "Water" ) ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path brick:hasSubstance ],
        [ sh:class brick:Equipment ;
            sh:description "Equipment that this point belongs to" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path brick:isPointOf ],
        [ sh:description "Physical quantity measured by this point" ;
            sh:in ( "Absolute_Humidity" "Acceleration_Time" "Active_Energy" "Active_Power" "Air_Quality" "Alternating_Current_Frequency" "Ammonia_Concentration" "Angle" "Apparent_Energy" "Apparent_Power" "Atmospheric_Pressure" "CO2_Concentration" "CO_Concentration" "Capacity" "Cloudage" "Complex_Power" "Conductivity" "Current_Angle" "Current_Imbalance" "Current_Total_Harmonic_Distortion" "Deceleration_Time" "Dewpoint" "Differential_CO2_Concentration" "Differential_CO_Concentration" "Differential_Dry_Bulb_Temperature" "Differential_Dynamic_Pressure" "Differential_Pressure" "Differential_Static_Pressure" "Differential_Temperature" "Direction" "Dry_Bulb_Temperature" "Dynamic_Pressure" "Electric_Current" "Electric_Energy" "Electric_Power" "Energy" "Enthalpy" "Flow" "Flow_Loss" "Formaldehyde_Concentration" "Frequency" "GrainsOfMoisture" "Humidity" "Illuminance" "Irradiance" "Level" "Linear_Speed" "Luminance" "Luminous_Flux" "Luminous_Intensity" "Mass" "Methane_Concentration" "NO2_Concentration" "Occupancy" "Occupancy_Count" "Occupancy_Percentage" "Operative_Temperature" "Ozone_Concentration" "PM10_Concentration" "PM1_Concentration" "PM2_5_Concentration" "Peak_Power" "Phasor" "Phasor_Angle" "Phasor_Magnitude" "Position" "Power" "Power_Factor" "Precipitation" "Pressure" "Radiance" "Radiant_Temperature" "Radioactivity_Concentration" "Radon_Concentration" "Reactive_Energy" "Reactive_Power" "Real_Power" "Relative_Humidity" "Rotational_Speed" "Solar_Irradiance" "Solar_Radiance" "Speed" "Static_Pressure" "TVOC_Concentration" "Temperature" "Thermal_Energy" "Thermal_Power" "Time" "Torque" "Velocity_Pressure" "Voltage" "Voltage_Angle" "Voltage_Imbalance" "Weather_Condition" "Wet_Bulb_Temperature" "Wind_Direction" ) ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path brick:hasQuantity ],
        [ sh:class sbco:KeyBoolMapEntry ;
            sh:description "map(string -> boolean)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path rec:customTags ] ;
    sh:targetClass brick:Point .

linkml:AnyShape a sh:NodeShape ;
    sh:closed false ;
    sh:description "Any resource or value." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass linkml:Any .

rec:AgentShape a sh:NodeShape ;
    sh:closed false ;
    sh:description "An entity that can act or be acted upon" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class rec:Organization ;
            sh:description "Indicates membership in an organization. Note that componency (e.g., departments of a corporation) are expressed using the more generic Organization.isPartOf property." ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path rec:memberOf ],
        [ sh:class sbco:KeyMapOfStringMapEntry ;
            sh:description "map(string -> map(string -> string))" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path rec:customProperties ],
        [ sh:class sbco:KeyStringMapEntry ;
            sh:description "map(string -> string)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path rec:identifiers ],
        [ sh:class sbco:KeyBoolMapEntry ;
            sh:description "map(string -> boolean)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path rec:customTags ],
        [ sh:class rdfs:Resource ;
            sh:description " Indicates ownership of some thing, e.g., a building, an asset, an organization, etc." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path rec:owns ],
        [ sh:datatype xsd:string ;
            sh:description "Machine or Human-readable name" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path rec:name ],
        [ sh:datatype xsd:string ;
            sh:description "Unique identifier within the schema. Must start with a letter and contain only letters, digits, underscores, hyphens, colons, semicolons, or periods. DTMI is one acceptable example." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path sbco:id ] ;
    sh:targetClass rec:Agent .

rec:ArchitectureAreaShape a sh:NodeShape ;
    sh:closed false ;
    sh:description "Describes business-relevant area measurements typically associated with architected spaces" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Machine or Human-readable name" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path rec:name ],
        [ sh:datatype xsd:string ;
            sh:description "Unique identifier within the schema. Must start with a letter and contain only letters, digits, underscores, hyphens, colons, semicolons, or periods. DTMI is one acceptable example." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path sbco:id ],
        [ sh:class sbco:KeyStringMapEntry ;
            sh:description "map(string -> string)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path rec:identifiers ],
        [ sh:class sbco:KeyMapOfStringMapEntry ;
            sh:description "map(string -> map(string -> string))" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path rec:customProperties ],
        [ sh:class sbco:KeyBoolMapEntry ;
            sh:description "map(string -> boolean)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path rec:customTags ] ;
    sh:targetClass rec:ArchitectureArea .

rec:ArchitectureCapacityShape a sh:NodeShape ;
    sh:closed false ;
    sh:description "Describes business-relevant capacity measurements typically associated with architected spaces" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class sbco:KeyMapOfStringMapEntry ;
            sh:description "map(string -> map(string -> string))" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path rec:customProperties ],
        [ sh:class sbco:KeyStringMapEntry ;
            sh:description "map(string -> string)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path rec:identifiers ],
        [ sh:datatype xsd:string ;
            sh:description "Unique identifier within the schema. Must start with a letter and contain only letters, digits, underscores, hyphens, colons, semicolons, or periods. DTMI is one acceptable example." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path sbco:id ],
        [ sh:datatype xsd:string ;
            sh:description "Machine or Human-readable name" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path rec:name ],
        [ sh:class sbco:KeyBoolMapEntry ;
            sh:description "map(string -> boolean)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path rec:customTags ] ;
    sh:targetClass rec:ArchitectureCapacity .

rec:ArchitectureShape a sh:NodeShape ;
    sh:closed false ;
    sh:description "A designed/landscaped (or potentially designed/landscaped) part of the physical world that has a 3D spatial extent. E.g., a building site, a building, levels within the building, rooms, etc." ;
    sh:ignoredProperties ( rdf:type rec:levelNumber ) ;
    sh:property [ sh:class rdfs:Resource ;
            sh:description "Resource that feeds this architecture" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 10 ;
            sh:path rec:isFedBy ],
        [ sh:class sbco:EquipmentExt ;
            sh:description "Subject is the physical location encapsulating the object." ;
            sh:nodeKind sh:IRI ;
            sh:order 17 ;
            sh:path rec:isLocationOf ],
        [ sh:class rec:Document ;
            sh:description "Documentation related to this asset" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 7 ;
            sh:path rec:documentation ],
        [ sh:class rec:ArchitectureArea ;
            sh:description "Area of the architecture" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path rec:area ],
        [ sh:description "Point associated with this architecture" ;
            sh:or ( [ sh:class brick:Point ] [ sh:class sbco:PointExt ] ) ;
            sh:order 8 ;
            sh:path rec:hasPoint ],
        [ sh:class sbco:KeyStringMapEntry ;
            sh:description "map(string -> string)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 21 ;
            sh:path rec:identifiers ],
        [ sh:class rec:Agent ;
            sh:description "Agent or resource that architected this structure" ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path rec:architectedBy ],
        [ sh:class rec:BuildingElement ;
            sh:description "Building element intersecting with this architecture" ;
            sh:nodeKind sh:IRI ;
            sh:order 9 ;
            sh:path rec:intersectingElement ],
        [ sh:maxCount 1 ;
            sh:or ( [ sh:class rec:Space ] [ sh:class rec:Site ] [ sh:class rec:Building ] [ sh:class rec:Level ] [ sh:class rec:Room ] [ sh:class rec:Zone ] [ sh:class rec:OutdoorSpace ] ) ;
            sh:order 18 ;
            sh:path rec:isPartOf ],
        [ sh:datatype xsd:string ;
            sh:description "Machine or Human-readable name" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 22 ;
            sh:path rec:name ],
        [ sh:class rec:Agent ;
            sh:description "Agent or resource that owns this architecture" ;
            sh:nodeKind sh:IRI ;
            sh:order 12 ;
            sh:path rec:ownedBy ],
        [ sh:class rec:Space ;
            sh:description "The subject is composed in part of the entity given by the object." ;
            sh:nodeKind sh:IRI ;
            sh:order 16 ;
            sh:path rec:hasPart ],
        [ sh:datatype xsd:string ;
            sh:description "A textual description of the resource" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 23 ;
            sh:path rec:description ],
        [ sh:class rec:BuildingElement ;
            sh:description "Building element contained within this architecture" ;
            sh:nodeKind sh:IRI ;
            sh:order 6 ;
            sh:path rec:containsElement ],
        [ sh:class rec:Agent ;
            sh:description "Agent or resource that operates this architecture" ;
            sh:nodeKind sh:IRI ;
            sh:order 11 ;
            sh:path rec:operatedBy ],
        [ sh:class rec:BuildingElement ;
            sh:description "Building element adjacent to this architecture" ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path rec:adjacentElement ],
        [ sh:class rec:Geometry ;
            sh:description "Polygon representing the spatial extent of this Space." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 14 ;
            sh:path rec:geometry ],
        [ sh:class sbco:KeyBoolMapEntry ;
            sh:description "map(string -> boolean)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 20 ;
            sh:path rec:customTags ],
        [ sh:class rec:Agent ;
            sh:description "Agent or resource that constructed this architecture" ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path rec:constructedBy ],
        [ sh:class rec:PostalAddress ;
            sh:description "Address of the architecture" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path rec:address ],
        [ sh:datatype xsd:string ;
            sh:description "Unique identifier within the schema. Must start with a letter and contain only letters, digits, underscores, hyphens, colons, semicolons, or periods. DTMI is one acceptable example." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 13 ;
            sh:path sbco:id ],
        [ sh:class rec:Georeference ;
            sh:description "A georeference creates a relationship between the local coordinate system used within a building (e.g., measured in meters) and a geographic coordinate system (e.g., lat, long, alt), such that locally placed Spaces can be resolved and rendered in that geographic coordinate system (e.g., for mapping purposes)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 15 ;
            sh:path rec:georeference ],
        [ sh:class sbco:KeyMapOfStringMapEntry ;
            sh:description "map(string -> map(string -> string))" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 19 ;
            sh:path rec:customProperties ],
        [ sh:class rec:ArchitectureCapacity ;
            sh:description "Capacity of the architecture" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path rec:capacity ] ;
    sh:targetClass rec:Architecture .

rec:AssetShape a sh:NodeShape ;
    sh:closed false ;
    sh:description "Something which is placed inside of a building, but is not an integral part of that building's structure; e.g., furniture, equipment, systems, etc." ;
    sh:ignoredProperties ( sbco:targetArea sbco:panel rdf:type sbco:installationArea brick:operationalStageCount brick:feeds sbco:deviceType rec:isFedBy ) ;
    sh:property [ sh:datatype xsd:date ;
            sh:description "Date when the asset was commissioned" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 14 ;
            sh:path rec:commissioningDate ],
        [ sh:datatype xsd:string ;
            sh:description "MAC address of the asset" ;
            sh:nodeKind sh:Literal ;
            sh:order 21 ;
            sh:path rec:MACAddress ;
            sh:pattern "^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})$" ],
        [ sh:datatype xsd:date ;
            sh:description "Date when the asset was turned over" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 26 ;
            sh:path rec:turnoverDate ],
        [ sh:description "Space where this asset is located" ;
            sh:or ( [ sh:class rec:Space ] [ sh:class rec:Site ] [ sh:class rec:Building ] [ sh:class rec:Level ] [ sh:class rec:Room ] [ sh:class rec:Zone ] [ sh:class rec:OutdoorSpace ] ) ;
            sh:order 9 ;
            sh:path rec:locatedIn ],
        [ sh:class rec:Agent ;
            sh:description "Agent or resource that commissioned this asset" ;
            sh:nodeKind sh:IRI ;
