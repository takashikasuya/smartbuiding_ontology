@prefix ex: <https://example.org/onto/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

xsd:integer a rdfs:Datatype .

<https://example.org/building-model.owl.ttl> a owl:Ontology ;
    rdfs:label "building_model" ;
    skos:definition """Smart-building oriented LinkML schema. Defines Site, Building, Level, Space, Equipment, Point and their relationships. This YAML is the single source of truth from which OWL/SHACL/JSON Schema/Docs are generated.
""" .

ex:Campus a owl:Class ;
    rdfs:label "Campus" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:sites ],
        [ a owl:Restriction ;
            owl:allValuesFrom ex:Site ;
            owl:onProperty ex:sites ] ;
    skos:definition "Top-level container for a campus with sites (optional helper class)." ;
    skos:inScheme <https://example.org/building-model> .

ex:Point a owl:Class ;
    rdfs:label "Point" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:custom_tags ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:enabled ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:boolean ;
            owl:onProperty ex:enabled ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:custom_tags ],
        [ a owl:Restriction ;
            owl:allValuesFrom ex:Equipment ;
            owl:onProperty ex:equipment ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:equipment ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:equipment ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:enabled ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:name ] ;
    skos:definition "A sensor, actuator, or data point associated with equipment." ;
    skos:inScheme <https://example.org/building-model> .

ex:buildings a owl:ObjectProperty ;
    rdfs:label "buildings" ;
    rdfs:range ex:Building ;
    skos:definition "Buildings located at this site" ;
    skos:inScheme <https://example.org/building-model> .

ex:equipment_list a owl:ObjectProperty ;
    rdfs:label "equipment_list" ;
    rdfs:range ex:Equipment ;
    skos:definition "Equipment installed in this space" ;
    skos:inScheme <https://example.org/building-model> .

ex:levels a owl:ObjectProperty ;
    rdfs:label "levels" ;
    rdfs:range ex:Level ;
    skos:definition "Levels (floors) in this building" ;
    skos:inScheme <https://example.org/building-model> .

ex:points a owl:ObjectProperty ;
    rdfs:label "points" ;
    rdfs:range ex:Point ;
    skos:definition "Points (sensors/actuators) connected to this equipment" ;
    skos:inScheme <https://example.org/building-model> .

ex:sites a owl:ObjectProperty ;
    rdfs:label "sites" ;
    rdfs:range ex:Site ;
    skos:definition "Sites that belong to this campus" ;
    skos:inScheme <https://example.org/building-model> .

ex:spaces a owl:ObjectProperty ;
    rdfs:label "spaces" ;
    rdfs:range ex:Space ;
    skos:definition "Spaces on this level" ;
    skos:inScheme <https://example.org/building-model> .

xsd:boolean a rdfs:Datatype .

ex:building a owl:ObjectProperty ;
    rdfs:label "building" ;
    rdfs:range ex:Building ;
    skos:definition "Building this level is part of" ;
    skos:inScheme <https://example.org/building-model> .

ex:equipment a owl:ObjectProperty ;
    rdfs:label "equipment" ;
    rdfs:range ex:Equipment ;
    skos:definition "Equipment associated with this point" ;
    skos:inScheme <https://example.org/building-model> .

ex:installed_in a owl:ObjectProperty ;
    rdfs:label "installed_in" ;
    rdfs:range ex:Space ;
    skos:definition "Space where this equipment is installed" ;        
    skos:inScheme <https://example.org/building-model> .

ex:level a owl:ObjectProperty ;
    rdfs:label "level" ;
    rdfs:range ex:Level ;
    skos:definition "Level this space is on" ;
    skos:inScheme <https://example.org/building-model> .

ex:site a owl:ObjectProperty ;
    rdfs:label "site" ;
    rdfs:range ex:Site ;
    skos:definition "Site this building belongs to" ;
    skos:inScheme <https://example.org/building-model> .

ex:Building a owl:Class ;
    rdfs:label "Building" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:custom_tags ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom ex:Level ;
            owl:onProperty ex:levels ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom ex:Site ;
            owl:onProperty ex:site ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:site ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:custom_tags ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:site ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:levels ] ;
    skos:definition "A building which is part of a site." ;
    skos:inScheme <https://example.org/building-model> .

ex:Equipment a owl:Class ;
    rdfs:label "Equipment" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:boolean ;
            owl:onProperty ex:enabled ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:installed_in ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:custom_tags ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:enabled ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:points ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:enabled ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom ex:Point ;
            owl:onProperty ex:points ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom ex:Space ;
            owl:onProperty ex:installed_in ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:installed_in ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:custom_tags ] ;
    skos:definition "An equipment/device installed in a space." ;      
    skos:inScheme <https://example.org/building-model> .

ex:Level a owl:Class ;
    rdfs:label "Level" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:building ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:spaces ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:custom_tags ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:custom_tags ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:building ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom ex:Space ;
            owl:onProperty ex:spaces ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom ex:Building ;
            owl:onProperty ex:building ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:name ] ;
    skos:definition "A level (floor) of a building." ;
    skos:inScheme <https://example.org/building-model> .

ex:Site a owl:Class ;
    rdfs:label "Site" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom ex:Building ;
            owl:onProperty ex:buildings ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:custom_tags ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:buildings ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:custom_tags ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:name ] ;
    skos:definition "A site or campus containing one or more buildings." ;
    skos:inScheme <https://example.org/building-model> .

ex:Space a owl:Class ;
    rdfs:label "Space" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:level ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:custom_tags ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:custom_tags ],
        [ a owl:Restriction ;
            owl:allValuesFrom ex:Level ;
            owl:onProperty ex:level ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom ex:Equipment ;
            owl:onProperty ex:equipment_list ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty ex:equipment_list ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty ex:id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty ex:level ] ;
    skos:definition "A space (room or area) within a level." ;
    skos:inScheme <https://example.org/building-model> .

ex:enabled a owl:DatatypeProperty ;
    rdfs:label "enabled" ;
    rdfs:range xsd:boolean ;
    skos:definition "Whether the entity is active/enabled" ;
    skos:inScheme <https://example.org/building-model> .

ex:custom_tags a owl:DatatypeProperty ;
    rdfs:label "custom_tags" ;
    rdfs:range xsd:string ;
    skos:definition "Free-form tags" ;
    skos:inScheme <https://example.org/building-model> .

    skos:inScheme <https://example.org/building-model> .

    skos:inScheme <https://example.org/building-model> .

ex:id a owl:DatatypeProperty ;
    rdfs:label "id" ;
    rdfs:range xsd:string ;
    skos:definition "A unique identifier (URI, GUID, or local id)" ;
    skos:inScheme <https://example.org/building-model> .

ex:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:range xsd:string ;
    skos:definition "Human-readable name" ;
    skos:inScheme <https://example.org/building-model> .

xsd:string a rdfs:Datatype .